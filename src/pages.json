{
  "easycom": {
    "custom": {
      "^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/manager/manage/index"
    },
    {
      "path": "pages/manager/approval/index"
    },
    {
      "path": "pages/manager/account/index"
    },
    {
      "path": "pages/student/home/index"
    },
    {
      "path": "pages/student/mine/index"
    },
    {
      "path": "pages/student/calendar/index"
    }
  ],
  "subPackages": [
    {
      "root": "pages/common",
      "pages": [{ "path": "login/index" }, { "path": "register/index" }]
    },
    {
      "root": "pages/manager/subPages",
      "pages": [
        { "path": "DormitoryInfo" },
        { "path": "ApprovalDetail" },
        { "path": "AccountInfo" }
      ]
    },
    {
      "root": "pages/student/subPages",
      "pages": [{ "path": "Notification" }]
    }
  ],
  "tabBar": {
    "custom": true,
    "list": [
      {
        "pagePath": "pages/manager/manage/index"
      },
      {
        "pagePath": "pages/manager/approval/index"
      },
      {
        "pagePath": "pages/manager/account/index"
      },
      {
        "pagePath": "pages/student/home/index"
      },
      {
        "pagePath": "pages/student/mine/index"
      },
      {
        "pagePath": "pages/student/calendar/index"
      }
    ]
  },
  "globalStyle": {
    "navigationStyle": "custom",
    "navigationBarTextStyle": "black"
  }
}
